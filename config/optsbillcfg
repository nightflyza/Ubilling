CHAPTER_STG=Stargazer setup
baseconf =VARCHAR|Stargazer interaction handler
SGCONF=VARCHAR|sgconf path
SGCONFXML=VARCHAR|sgconf_xml path
STG_HOST=VARCHAR|Stargazer host
STG_PORT=VARCHAR|Stargazer port
XMLRPC_PORT=VARCHAR|XML RPC port
STG_LOGIN=VARCHAR|Stargazer admin login
STG_PASSWD=VARCHAR|Stargazer admin password|PASSWD
STGPID=VARCHAR|stargazer PID path
STGNASHUP=TRIGGER|reload stargazer configuration on NAS operations
RESET_AO=TRIGGER|Use AlwaysOnline instead Down flag to reset users
CHAPTEREND_STG=true

CHAPTER_PATHS=External programs
SUDO=VARCHAR|sudo path
TOP = VARCHAR|batch top path
CAT=VARCHAR|cat path
GREP=VARCHAR|grep path
RC_DHCPD=VARCHAR|isc-dhcpd init path
UPTIME=VARCHAR|uptime path
PING=VARCHAR|ping path
TAIL=VARCHAR|tail path
KILL=VARCHAR|kill path
PHPSYSINFO=VARCHAR|PHPSysInfo path
CHAPTEREND_PATHS=true

CHAPTER_OTHER=Misc options
LANG =VARCHAR|Default language
TASKBAR_ICON_SIZE =VARCHAR|Taskbar module icon size
REGRANDOM_MAC=TRIGGER|Register new users with random MAC
REGALWONLINE=TRIGGER|Register new users with AlwaysOnline flag
REGDISABLEDSTAT=TRIGGER|Register users with disabled detailed stats
CHAPTEREND_OTHER=true